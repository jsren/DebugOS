<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="DebugOS.ConfigCategoryItem"
	x:Name="UserControl"
	d:DesignWidth="189" d:DesignHeight="38" MouseLeftButtonUp="OnSelected">

	<Grid Background="#FFF4F4F5">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="21*"/>
			<ColumnDefinition Width="73*"/>
		</Grid.ColumnDefinitions>
        <TextBlock x:Name="headerText" HorizontalAlignment="Left" Margin="6,0" TextWrapping="Wrap" VerticalAlignment="Center" FontWeight="Bold" Foreground="#FF4D4D4D" Grid.Column="1"><Run Language="en-gb" Text="&lt;untitled&gt;"/></TextBlock>
    </Grid>
</UserControl>