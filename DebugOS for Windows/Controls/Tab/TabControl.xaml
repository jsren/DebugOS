<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:DebugOS"
	mc:Ignorable="d"
	x:Class="DebugOS.TabControl"
	x:Name="UserControl"
	d:DesignWidth="372" d:DesignHeight="110">

	<Grid x:Name="LayoutRoot">
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="Auto"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<ContentPresenter x:Name="content" Margin="0" Grid.Row="2"/>
		<ScrollViewer x:Name="tabScroll" Grid.Row="1" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Disabled" Template="{DynamicResource TabControlScroller}" >
			<StackPanel x:Name="tabStack" Orientation="Horizontal" Height="30" HorizontalAlignment="Left">
			</StackPanel>
		</ScrollViewer>
	</Grid>
</UserControl>