<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="border" Color="#FF8C8E94"/>
	<Color x:Key="PrimaryColour">#FF628BAE</Color>
	<Color x:Key="WarningColour">#FFFFBD90</Color>
	<Color x:Key="PrimaryLabel">White</Color>
	<Color x:Key="WarningLabel">Black</Color>
	<Color x:Key="SecondaryColour">#FF99B4D1</Color>
	<Color x:Key="StatusNotConnected">#FF636363</Color>
	<Color x:Key="StatusWarn">#FFA6593D</Color>
	<Color x:Key="StatusGood">#FF628BAE</Color>
	<Color x:Key="StatusLabel">#FF151515</Color>
	<Color x:Key="Background">White</Color>
    <Color x:Key="StackReturnItem">#FFB9C2E5</Color>
    <Color x:Key="TabLabel">#FF272727</Color>
    <Color x:Key="TabBackground">#FFF3F3F3</Color>
    <Color x:Key="DialogTitle">#FF303030</Color>
