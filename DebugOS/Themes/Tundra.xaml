<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="border" Color="#FF8C8E94"/>
	<Color x:Key="PrimaryColour">#FF2E2E2E</Color>
	<Color x:Key="WarningColour">#FFFF3535</Color>
	<Color x:Key="PrimaryLabel">White</Color>
	<Color x:Key="WarningLabel">Black</Color>
	<Color x:Key="SecondaryColour">#FF99B4D1</Color>
	<Color x:Key="StatusNotConnected">#FF636363</Color>
	<Color x:Key="StatusWarn">#FFA6593D</Color>
	<Color x:Key="StatusGood">#FF358934</Color>
	<Color x:Key="StatusLabel">#FF151515</Color>
	<Color x:Key="Background">#FF464646</Color>
	<!-- Resource dictionary entries should be defined here. -->
</ResourceDictionary>