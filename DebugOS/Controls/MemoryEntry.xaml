<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="DebugOS.MemoryEntry"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480">
	<Grid x:Name="LayoutRoot" HorizontalAlignment="Left" Height="22.5" VerticalAlignment="Top" Width="217.5">
		<Rectangle Fill="#FFF1F1F1"/>
		<Rectangle HorizontalAlignment="Left" Width="27.084" Fill="#FFD6D6D6" Margin="1,1,0,1"/>
		<TextBlock x:Name="addressLabel" Margin="34.334,2.878,0,3.622" TextWrapping="Wrap" Foreground="#FF585858" HorizontalAlignment="Left"><Run Language="en-gb" Text="0x00000000"/></TextBlock>
		<Grid Margin="106.75,2.001,5.3,2.499" ClipToBounds="True">
			<TextBox x:Name="valueInput" TextWrapping="Wrap" BorderBrush="{x:Null}" Foreground="#FF646464" Margin="-2" Padding="3,1,3,3" Background="#FFF7F7F7"/>
		</Grid>
	</Grid>
</UserControl>